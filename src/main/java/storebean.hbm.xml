<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.usastores.xml.bean.StoreBean" table="storetable">
		<id name="storeId">
			<column name="store_Id"></column>
			<generator class="increment"></generator>
		</id>
		<property name="storeName" column="store_name">
		</property>
		<property name="storeArea" column="store_Area">
		</property>
		<property name="zipcode" column="store_zipcode">
		</property>
		<property name="distanceInMiles" column="store_distance">
		</property>
	</class>
</hibernate-mapping>